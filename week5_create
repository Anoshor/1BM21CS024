create table dept(deptno int, dname varchar(20), dloc varchar(30), primary key (deptno));
create table employee (empno int, ename varchar(30), mgr_no int, hiredate date, sal float, deptnum int, primary key (empno), foreign key (deptnum) references dept (deptnum) on delete cascade on update cascade);
create table project (pno int, ploc varchar(30), pname varchar(30), primary key (pno));
create table incentives (empno int,incentive_date date, incentive_amount float, primary key (incentive_date) foreign key (empno) references employee (empno) on delete cascade on update cascade);
create table assigned_to (empno int, pno int, job_role varchar(20), foreign key (empno) references employee (empno) on delete cascade on update cascade, foreign key (pno) references project (pno) on delete cascade on update cascade);

insert into dept values (1,'CS','Delhi');
insert into dept values (2,'IS','');
insert into dept values (3,'ME','Delhi');
insert into dept values (4,'CE','Delhi');
insert into dept values (5,'AS','Delhi');

select a.empno from assigned_to a, project p where (p.pno=a.pno) and (p.ploc in ('Benagluru','Hyderabad','Mysore');

incentives-get : select empno from from incentives

select empno from employees where empno not in (incentives-get);

select e.ename, e.empno, d.dname, a.job_role, p.ploc, d.dloc from dept d, employee e, project p, incentive i, assigned_to a where (p.ploc=d.dloc);
